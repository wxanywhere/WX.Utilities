<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
                    xmlns:v="clr-namespace:WX.Utilities.Downloader.View"
                    xmlns:vm="clr-namespace:WX.Utilities.Downloader.ViewModel;assembly=WX.Utilities.Downloader.AppLogic"
                    >
  <DataTemplate DataType="{x:Type vm:FVM_DownloadingTask}">
    <v:UC_DownloadingTask />
  </DataTemplate>
  <DataTemplate DataType="{x:Type vm:FVM_FinishedTaskes}">
    <v:UC_FinishedTaskes />
  </DataTemplate>
  <DataTemplate DataType="{x:Type vm:FVM_NewDownloadTask}">
    <v:UC_NewDownloadTask />
  </DataTemplate>
</ResourceDictionary>