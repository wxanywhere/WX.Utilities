    <!-- =============================================================== -->
    <cv:ConverterBoolToVisibility x:Key="ConverterBoolToVisibility" />
    <Style x:Key="ButtonDefaultStyleWX" TargetType="{x:Type Button}" BasedOn="{StaticResource {x:Type Button}}">
    </Style>
    <Style x:Key="TextBoxDefaultStyleWX" TargetType="{x:Type TextBox}" BasedOn="{StaticResource {x:Type TextBox}}">
        <Setter  Property="Foreground" Value="Black"/>
    </Style>
    <Style x:Key="LabelDefaultStyleWX" TargetType="{x:Type Label}" BasedOn="{StaticResource {x:Type Label}}">
        <Setter  Property="Foreground" Value="Black"/>
    </Style>
